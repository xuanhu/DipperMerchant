<?xml version="1.0" encoding="UTF-8"?>
<resources xmlns:android="http://schemas.android.com/apk/res/android">
  <declare-styleable name="ActivityHeaderView">
        <attr name="titleTextSize" format="dimension"/>
        <attr name="rightViewRes" format="reference"/>
        <attr name="titleText" format="string"/>
        <attr name="titleTextColor1" format="reference"/>
        <attr name="hideBackButton" format="boolean"/>
    </declare-styleable>
    <declare-styleable name="RotateProgress">
        <attr name="progress_color"  format="reference"/>
        <attr name="size" format="dimension"/>
    </declare-styleable>
    <declare-styleable name="PullRefreshListView">
        <attr name="divider"  format="reference"/>
        <attr name="dividerHeight"  format="dimension"/>
        <attr name="background_color"  format="reference"/>
    </declare-styleable>
    <declare-styleable name="CustomStar">
        <attr name="level" format="float"/>
    </declare-styleable>
    <declare-styleable name="ImageViewGroup">
        <attr name="column_count"  format="integer"/>
    </declare-styleable>
</resources>